{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "prettier"
  ],
  "plugins": [
    "jest",
    "prettier"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "ignorePatterns": [
    "dist/**/*",
    "jest.config.js"
  ],
  "rules": {
    "linebreak-style": "off",
    "prettier/prettier": "error"
  }
}